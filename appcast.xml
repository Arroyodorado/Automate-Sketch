<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:sparkle="http://www.andymatuschak.org/xml-namespaces/sparkle" xmlns:dc="http://purl.org/dc/elements/1.1/">
    <channel>
        <title>Automate Sketch</title>
        <link>http://ashung.github.io/Automate-Sketch/appcast.xml</link>
        <description>Make your workflow more efficient.</description>
        <language>en</language>
        {% for item in site.data.release %}
        <item>
            <title>{{ item.title }}</title>
            <description>
                <![CDATA[
                    {{ item.description }}
                ]]>
            </description>
            <enclosure
                url="https://github.com/Ashung/Automate-Sketch/archive/{{ item.commitID }}.zip"
                sparkle:version="{{ item.version }}"
                type="application/octet-stream"/>
        </item>
        {% endfor %}
    </channel>
</rss>
